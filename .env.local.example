# Lighter (testnet) configuration
# Base URL (confirm your target): e.g. https://testnet.zklighter.elliot.ai
LIGHTER_BASE_URL=

# Lighter API key index (usually 0)
LIGHTER_API_KEY_INDEX=0

# Lighter API key (public/private)
LIGHTER_API_KEY_PUBLIC=
LIGHTER_API_KEY_PRIVATE=

# Ethereum private key used for signing (no 0x prefix). DO NOT COMMIT REAL KEYS.
ETH_PRIVATE_KEY=

# Wallet address to receive/exchange funds (public address is safe to store)
LIGHTER_WALLET_ADDRESS=

# Backpack (optional)
BACKPACK_API_KEY=
BACKPACK_API_SECRET=

# Optional: Gemini or OpenAI keys if used
GOOGLE_API_KEY=
OPENAI_API_KEY=
